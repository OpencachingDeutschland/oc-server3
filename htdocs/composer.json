{
  "name": "opencaching/oc-server",
  "license": "GNU General Public License",
  "description": "opencaching.de source code",
  "require": {
    "php": ">=7.2.5",
    "ezyang/htmlpurifier": "^v4.8",
    "smarty/smarty": "~2.6",
    "symfony/symfony": "4.4.*",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-cache-bundle": "^1.2",
    "symfony/swiftmailer-bundle": "^3.4",
    "symfony/monolog-bundle": "^3.5",
    "symfony/polyfill-apcu": "^1.0",
    "sensio/framework-extra-bundle": "^5.0",
    "doctrine/doctrine-migrations-bundle": "^1.3",
    "twig/extensions": "^1.3",
    "jms/translation-bundle": "^1.2",
    "beberlei/doctrineextensions": "^1.0",
    "leafo/scssphp": "^v0.8",
    "league/commonmark": "^0.18.3",
    "sentry/sentry": "^1.7",
    "opencaching/okapi": "dev-master",
    "endroid/qr-code": "^3.5.0",
    "twbs/bootstrap": "^v4.1.0",
    "symfony-cmf/menu-bundle": "^2.1",
    "components/jquery": "^3.2",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "ext-simplexml": "*",
    "ext-posix": "*",
    "ext-curl": "*",
    "ext-zlib": "*",
    "eluceo/ical": "^0.15.0",
    "league/csv": "^9.6",
    "mervick/material-design-icons": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "6.*",
    "behat/behat": "^3.5",
    "behat/mink": "dev-master",
    "behat/mink-extension": "^2.3",
    "behat/mink-goutte-driver": "^1.2",
    "squizlabs/php_codesniffer": "^2.6",
    "fig-r/psr2r-sniffer": "^0.3.0",
    "codedungeon/phpunit-result-printer": "^0.4.4",
    "fzaninotto/faker": "^1.8",
    "nette/php-generator": "^2.6"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/opencaching/okapi"
    }
  ],
  "autoload": {
    "psr-4": {
      "OcTest\\": "../tests",
      "Oc\\": "src/Oc",
      "": "src/"
    },
    "classmap": [
      "lib2/",
      "lib2/logic/",
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*"
    }
  }
}
